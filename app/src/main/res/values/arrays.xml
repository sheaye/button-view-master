<?xml version="1.0" encoding="utf-8"?>
<resources>
    <array name="drawables">
        <item>@drawable/ic_normal</item>
        <item>@drawable/ic_pressed</item>
        <item>@drawable/ic_selected</item>
    </array>

    <array name="solidColors">
        <item>@color/colorLightGray</item>
        <item>@color/colorDarkGray</item>
        <item>@color/colorRed</item>
    </array>

    <array name="textColors">
        <item>@color/colorBlack</item>
        <item>@color/colorLightBlack</item>
        <item>@color/colorWhite</item>
    </array>

    <array name="strokeColors">
        <item>@color/colorBlack</item>
        <item>@color/colorLightBlack</item>
        <item>@color/colorPrimaryDark</item>
    </array>

    <array name="textColorForCompound">
        <item>@color/colorPrimary</item>
        <item>@color/colorPrimaryDark</item>
        <item>@color/colorAccent</item>
    </array>

    <array name="shapes">
        <item>rectangle</item>
        <item>circleRectangle</item>
        <item>circle</item>
    </array>

    <string-array name="states">
        <item>normal</item>
        <item>normal and pressed</item>
        <item>normal, pressed and selected</item>
    </string-array>

    <string-array name="cornerRadius">
        <item>5</item>
        <item>10</item>
        <item>15</item>
    </string-array>

</resources>